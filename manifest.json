{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"FFXIV Hunts",
		"version":"1.0.0",
		"minimum-overwolf-version":"0.77.10",
		"author":"Lima",
		"icon":"IconMouseOver.png",
		"icon_gray":"IconMouseNormal.png",
		"description":"Tracking hunts from ffxivhunt.com"
	},
	"data": {
		"start_window":"MainWindow",
		"windows":{
			"MainWindow":{
				"file":"index.html",
				"transparent":true,
				"resizable":true,
				"size":{"width":700, "height":300},
				"min_size":{"width":700, "height":200},
				"max_size":{"width":700, "height":500}
			}
		}
	}
}
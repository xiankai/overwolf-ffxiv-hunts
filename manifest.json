{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"FFXIV Hunts",
		"version":"1.0.0",
		"minimum-overwolf-version":"0.77.10",
		"author":"Lima",
		"icon":"icon.png",
		"icon_gray":"icon.png",
		"description":"Tracking hunts from ffxivhunt.com for Jenova"
	},
	"data": {
		"start_window":"MainWindow",
		"windows":{
			"MainWindow":{
				"file":"index.html",
				"transparent":true,
				"resizable":true,
				"size":{"width":700, "height":500},
				"min_size":{"width":700, "height":200}
			}
		}
	}
}